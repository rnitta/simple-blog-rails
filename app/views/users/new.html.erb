<h1>Users#new</h1>
<%= form_for @user,:url => {:action => :new} do |f| %>
  <%= f.label :name, '名前' %>
  <%= f.text_field :name %>
  <%= f.label :email, 'メールアドレス' %>
  <%= f.text_field :email %>
  <%= f.label :password, 'パスワード' %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation, '確認' %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit "作成" %>
<% end %>
